{
   "name": "wasaby-hello-world",
   "version": "22.7000.0",
   "description": "Simple Wasaby Hello World",
   "repository": {
      "type": "git",
      "url": "git@github.com:saby/wasaby-hello-world.git"
   },
   "scripts": {
      "build": "npm run build:typescript && npm run build:app",
      "build:typescript": "wasaby-cli --tasks=prepare",
      "build:app": "wasaby-cli --tasks=build",
      "build:watch": "wasaby-cli --tasks=build --watcher",
      "start": "wasaby-cli startServer",
      "prepare": "npm run update-cli-store && npm run build:typescript",
      "update-cli-store": "wasaby-cli --tasks=initStore"
   },
   "wasaby-cli": {
      "port": 777,
      "workDir": "./wasaby"
   },
   "dependencies": {},
   "devDependencies": {
      "@tensor-corp/eslint-config": "^2.0.3",
      "concurrently": "^5.3.0",
      "eslint": "^5.7.0",
      "stylelint": "^9.7.1",
      "stylelint-config-standard": "^18.2.0",
      "tslint": "^5.18.0",
      "wasaby-cli": "git+https://git.sbis.ru/saby/wasaby-cli.git#rc-22.7000"
   },
   "author": "Мустафин Л.И."
}
